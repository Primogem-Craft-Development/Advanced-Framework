{
  "schemaVersion": 1,
  "id": "advancedfmk",
  "version": "${version}",
  "name": "Advanced Framework",
  "description": "A modern render framework for rendering ui and 3d models & shaders",
  "authors": [
    "Jack253-png",
    "hackermdch"
  ],
  "contact": {
    "repo": "https://github.com/Primogem-Craft-Development/Advanced-Framework",
    "sources": "https://github.com/Primogem-Craft-Development/Advanced-Framework",
    "issues": "https://github.com/Primogem-Craft-Development/Advanced-Framework/issues"
  },
  "license": "GPLv3",
  "icon": "assets/advancedfmk/icon.png",
  "environment": "*",
  "accessWidener": "META-INF/aw.accesswidener",
  "entrypoints": {
    "client": [
      "com.primogemstudio.advancedfmk.client.AdvancedFrameworkClient"
    ],
    "main": [
      "com.primogemstudio.advancedfmk.AdvancedFramework"
    ],
    "preLaunch": [
      "com.primogemstudio.advancedfmk.RenderDocHook"
    ]
  },
  "mixins": [
    "advancedfmk.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.11.0",
    "fabric": "*",
    "minecraft": "1.20.1"
  }
}
